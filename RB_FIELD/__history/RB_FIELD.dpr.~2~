program RB_FIELD;

uses
  Vcl.Forms,
  P_RB_FIELD in 'P_RB_FIELD.pas' {Form1},
  DMGERAL in 'DMGERAL.pas' {DM: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TDM, DM);
  Application.Run;
end.
